{
  "containerName": "blender-framesplitter",
  "prePushHook": {
    "cmdList": [
      "echo \"Greetings. about to build ðŸ«¡ \"",
    ]
  },
  "postPushHook": {
    "k8s": {
      "replicas": 1,
      "minReadySeconds": 5, 
      "containerPort": 8080,
      "servicePort": 80,
      "nodePort": 31000,
    },
    "pipeline": {
      "input": {
        "pfs": {
          "glob": "/*",
          "repo": "blends"
        }
      },
      "transform": {
        "cmd": ["python3", "/splitter.py", "process", "blends"],
      }
    }
  }
}

